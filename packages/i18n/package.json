{
  "name": "@/i18n",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "codegen": "i18next-resources-for-ts interface -i ./src/locales/en-US -o ./src/@types/resources.d.ts",
    "postcodegen": "pnpm run format --write ./src/@types/resources.d.ts ./src/locales/**/*.json"
  },
  "devDependencies": {
    "@/eslint-config": "workspace:*",
    "@/prettier-config": "workspace:*",
    "@/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:eslint",
    "prettier": "catalog:prettier",
    "typescript": "catalog:"
  },
  "prettier": "@/prettier-config",
  "dependencies": {
    "@t3-oss/env-core": "catalog:",
    "discord.js": "^14.21.0",
    "i18next": "^23.11.5",
    "i18next-resources-for-ts": "^1.5.0"
  }
}
