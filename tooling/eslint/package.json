    "eslint-plugin-react-hooks": "catalog:react19",
    "eslint-plugin-react-refresh": "catalog:react19",
    "eslint-plugin-turbo": "catalog:eslint",
    "typescript-eslint": "catalog:eslint"
  },
  "devDependencies": {
    "@/prettier-config": "workspace:*",
    "@/tsconfig": "workspace:*",
    "eslint": "catalog:eslint",
    "prettier": "catalog:prettier",
    "typescript": "catalog:"
